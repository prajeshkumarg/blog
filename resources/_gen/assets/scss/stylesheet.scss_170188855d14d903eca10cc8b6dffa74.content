html,body{font-family:Helvetica,sans-serif;width:100%;height:100%;margin:0;padding:0;justify-content:center;display:flex;background-color:#201e1f}@media(min-width:0px){.container{min-height:100%;min-width:95%;max-width:95%;display:grid;grid-template-rows:auto 1fr auto;grid-template-columns:100%}.nav-container{display:flex;flex-direction:column;align-items:start;font-size:1.5rem;padding:16px;width:100%}.nav-container a{text-decoration:none;color:#faebd7}.nav-container a:hover,.nav-container a:focus{font-style:bold;color:#e0ffff}.nav-left,.nav-right{display:flex;flex-direction:row;justify-content:space-between}.nav-left{font-weight:700}.nav-left a{letter-spacing:0}.nav-right{width:40%;font-size:1.2rem;align-items:center;justify-content:space-between;margin-top:1em}.main-container{color:#faebd7;padding:16px}.main-construction{display:flex;justify-content:start;align-items:center}.about{margin:4em 0;text-align:left;line-height:1.5;font-size:1rem}.about .about-question{font-style:italic;font-size:.9rem;color:#e0ffff}.about .about-highlight{font-weight:700;font-size:1.2rem;text-align:left}.about ul{padding:0;list-style:none}.about a{text-decoration:none;color:#add8e6;font-style:italic}.about a:hover{font-style:bold;color:#faebd7}.main{margin:4em 0;text-align:left;line-height:1.5}.main .main-emoji{font-size:2rem}.main .main-subscript{font-size:1.2rem;text-align:start}.main-post{margin:4em 0;text-align:justify;line-height:1.7;font-size:.6rem}.main-post code{white-space:pre-line;font-size:1rem;font-style:italic}.main-post .table-container{display:flex;justify-content:space-around;max-width:100%;overflow:hidden}.main-post .table-container .table-col{padding:4px;max-height:45%;max-width:45%}.main-post h1{font-size:1.5rem}.main-post p{font-size:1.2rem}.main-post li{font-size:1.2rem}.main-post a{color:#d3d3d3}.videowrapper{float:none;clear:both;width:100%;position:relative;padding-bottom:56.25%;padding-top:25px;height:0}.videowrapper iframe{position:absolute;top:0;left:0;width:100%;height:100%}.footer-container{color:#d3d3d3;place-content:start;display:flex;padding:16px}.footer-container a{color:#d3d3d3}}@media(min-width:768px){.container{min-width:75%;max-width:75%}.nav-container{flex-direction:row;justify-content:center;width:auto;justify-content:space-between}.nav-left,.nav-right{flex-direction:row;align-items:center;margin-top:0;font-size:1.5rem;justify-content:space-between}.main-construction .main{width:100%}.main-construction .about{width:100%;font-size:1rem}.about{font-size:1.5rem}.about .about-question{font-size:1.1rem}.about .about-highlight{font-size:2rem}.about a{font-size:1.3rem}.main-post{width:auto;font-size:1.1rem}.main-post h1{font-size:2.5rem}.main-post .table-container .table-col{max-height:30%;max-width:30%}.footer-container{place-content:center}}@media(min-width:1080px){.container{min-width:40%;max-width:40%}.main-construction p{font-size:2rem}}