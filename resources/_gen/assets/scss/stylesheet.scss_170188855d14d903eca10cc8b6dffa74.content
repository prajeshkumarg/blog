html,body{font-family:Helvetica,sans-serif;width:100%;height:100%;margin:0;padding:0;justify-content:center;display:flex;background-color:#201e1f}@media(min-width:0px){.container{min-height:100%;min-width:95%;max-width:95%;display:grid;grid-template-rows:auto 1fr auto;grid-template-columns:100%}.nav-container,nav{display:flex;flex-direction:column;align-items:start;font-size:1.5rem;padding:16px;width:auto}.nav-container a,nav a{text-decoration:none;color:#faebd7}.nav-container a:hover,.nav-container a:focus,nav a:hover,nav a:focus{font-style:bold;color:#e0ffff}.nav-left,.nav-right{display:flex;flex-direction:row;justify-content:space-between}.nav-left{font-weight:700}.nav-left a{letter-spacing:0}.nav-right{width:40%;font-size:1.2rem;align-items:center;justify-content:space-between;margin-top:1em}.main-container{color:#faebd7;padding:16px}.main-construction{display:flex;justify-content:start;align-items:center;min-height:100%}.main-construction p{font-size:1.3rem}.footer-container{color:#d3d3d3;place-content:start;display:flex;padding:16px}.footer-container a{color:#d3d3d3}}@media(min-width:768px){.container{min-width:80%;max-width:80%}.nav-container,nav{flex-direction:row;justify-content:center;width:auto;justify-content:space-between;margin:0 16px}.nav-left,.nav-right{flex-direction:row;align-items:center;margin-top:0;font-size:2rem}.nav-left a,.nav-right a{margin-top:1em}.nav-right a{margin-left:2em}.main-post{padding:16px;margin:0 16px;text-align:justify;line-height:1.7}.main-post code{white-space:pre-line;font-size:1rem;font-style:italic}.main-post .table-container{display:flex;justify-content:space-around;max-width:100%;overflow:hidden}.main-post .table-container .table-col{padding:4px;max-height:30%;max-width:30%}.main-post iframe{max-width:100%;max-height:100%;align-items:center}.main-post p{font-size:1.2rem}.main-post a{color:#d3d3d3}}@media(min-width:1080px){.container{min-width:50%;max-width:50%}.main-construction p{font-size:2rem}}