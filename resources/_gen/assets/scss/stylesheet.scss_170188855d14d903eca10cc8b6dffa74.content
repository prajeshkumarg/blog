html,body{font-family:Helvetica,sans-serif;width:100%;height:100%;margin:0;padding:0;justify-content:center;display:flex;background-color:#201e1f}.container{min-height:100%;min-width:60%;max-width:60%;display:grid;grid-template-rows:auto 1fr auto;grid-template-columns:100%}.nav-container,nav{display:flex;justify-content:center;width:100%}nav{width:100%;padding:16px;justify-content:space-between;margin:0 16px}nav a{text-decoration:none;color:#faebd7}nav a:hover{font-style:bold;color:#e0ffff}.nav-left,.nav-right{font-size:1.5rem}.nav-right a{margin-left:2em}.main-container{color:#faebd7}.main-construction{display:flex;justify-content:center;align-items:center;min-height:100%}.main-construction p{font-size:2rem}.main-post{padding:16px;margin:0 16px;text-align:justify;line-height:1.7}.main-post code{white-space:pre-line;font-size:1rem;font-style:italic}.main-post .table-container{display:flex;justify-content:space-around;max-width:100%;overflow:hidden}.main-post .table-container .table-col{padding:4px;max-height:30%;max-width:30%}.main-post iframe{max-width:100%;max-height:100%;align-items:center}.main-post p{font-size:1.2rem}.main-post a{color:#d3d3d3}.footer-container{color:#d3d3d3;place-content:center;display:flex}.footer-container a{color:#d3d3d3}